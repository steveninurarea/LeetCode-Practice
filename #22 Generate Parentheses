class Solution(object):
    def generateParenthesis(self, n):
        """
        :type n: int
        :rtype: List[str]
        """
        res = []
        self.generate(res, n, n, '')
        return res
    
    def generate(self, res, left, right, path) :
        if left == 0 and right == 0 :
            res.append(path)
            return
        if left > 0 :
            self.generate(res, left-1, right, path+'(')
        if right > left :
            self.generate(res, left, right-1, path+')')
