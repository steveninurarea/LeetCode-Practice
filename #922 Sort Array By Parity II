class Solution(object):
    def sortArrayByParityII(self, nums):
        """
        :type nums: List[int]
        :rtype: List[int]
        """
        res = [0] * len(nums)
        odd = 1
        even = 0
        
        for i in range(len(nums)) :
            if nums[i] %2 == 0 :
                res[even] = nums[i]
                even += 2
            else :
                res[odd] = nums[i]
                odd += 2
        return res
