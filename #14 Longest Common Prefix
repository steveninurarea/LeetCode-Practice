class Solution(object):
    def longestCommonPrefix(self, strs):
        """
        :type strs: List[str]
        :rtype: str
        """
        if len(strs) == 0 :
            return ""
        c = strs[0]
        for i in range(1, len(strs)) :
            t = ""
            for j in range(len(strs[i])) :
                if strs[i][j] == c[j] :
                    t += c[j]
                else :
                    break
        return t
