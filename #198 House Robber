class Solution(object):
    def rob(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        odd = 0
        even = 0
        for i in range(len(nums)) :
            if i%2 == 0 :
                odd += nums[i]
            else :
                even += nums[i]
        return max(odd, even)
